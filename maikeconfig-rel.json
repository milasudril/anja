{
"target_hooks":[
	{"config":{
		"appcompile":{
			"args":[
				"-O3",
				"-fpic",
				"-fno-PIE",
				"-ffast-math",
				"-fno-stack-protector",
				"{cxxversion}",
				"-Wall",
				"{cflags_extra}",
				"{iquote}",
				"{includedir}",
				"-DMAIKE_TARGET_DIRECTORY={target_directory}",
				"-DMAIKE_CURRENT_DIRECTORY={current_directory}",
				"{includes_extra}",
				"-s",
				"-o",
				"{target}",
				"{source}",
				"{libdir}",
				"{dependencies}"
			],
			"name":"g++"
		},
		"dllcompile":{
			"args":[
				"-O3",
				"-fpic",
				"-ffast-math",
				"-fno-stack-protector",
				"{cxxversion}",
				"-Wall",
				"{cflags_extra}",
				"{iquote}",
				"{includedir}",
				"-DMAIKE_TARGET_DIRECTORY={target_directory}",
				"-DMAIKE_CURRENT_DIRECTORY={current_directory}",
				"{includes_extra}",
				"-shared",
				"-s",
				"-o",
				"{target}",
				"{source}",
				"{libdir}",
				"{dependencies}"
			],
			"name":"g++"
		},
		"objcompile":{
			"args":[
				"-O3",
				"-fno-stack-protector",
				"-c",
				"-fpic",
				"-ffast-math",
				"{cxxversion}",
				"-Wall",
				"{cflags_extra}",
				"{iquote}",
				"{includedir}",
				"-DMAIKE_TARGET_DIRECTORY={target_directory}",
				"-DMAIKE_CURRENT_DIRECTORY={current_directory}",
				"{includes_extra}",
				"-o",
				"{target}",
				"{source}"
			],
			"name":"g++"
		}
	},
	"filename_exts":[],
	"name":"targetcxx_default",
	"plugin":"targetcxx"
	}]
}
